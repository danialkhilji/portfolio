---
interface Props {
  label: string;
}

const { label } = Astro.props;
---

<span
  class="inline-block bg-white text-black border border-gray-500 px-3.5 py-1.5 m-1.5 rounded-full font-medium cursor-default transition-colors duration-300 hover:bg-brand-cyan hover:text-white hover:border-brand-cyan"
>
  {label}
</span>
